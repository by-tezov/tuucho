import com.tezov.tuucho.core.data.repository.database.type.Lifetime;
import com.tezov.tuucho.core.data.repository.database.type.Visibility;

CREATE TABLE hookEntry (
    primaryKey INTEGER PRIMARY KEY AUTOINCREMENT,
    url TEXT NOT NULL UNIQUE,
    rootPrimaryKey INTEGER REFERENCES jsonObjectCommonEntry(primaryKey),
    visibility TEXT AS Visibility NOT NULL,
    lifetime TEXT AS Lifetime NOT NULL
);
