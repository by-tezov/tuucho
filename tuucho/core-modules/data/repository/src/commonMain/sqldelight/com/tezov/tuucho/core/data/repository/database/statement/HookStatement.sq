selectAll:
SELECT * FROM hookEntry;

deleteAll:
DELETE FROM hookEntry;

deleteByUrl:
DELETE FROM hookEntry WHERE url = :url;

insert:
INSERT INTO hookEntry(url, urlWhiteList, rootPrimaryKey, visibility, lifetime)
VALUES (:url, :urlWhiteList, :rootPrimaryKey, :visibility, :lifetime);

getByUrl:
SELECT * FROM hookEntry WHERE url = :url;

getLifetimeByUrl:
SELECT lifetime FROM hookEntry WHERE url = :url;
