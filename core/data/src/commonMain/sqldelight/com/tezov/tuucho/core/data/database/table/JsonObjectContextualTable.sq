import kotlinx.serialization.json.JsonObject;

CREATE TABLE jsonObjectContextualEntry (
    primaryKey INTEGER PRIMARY KEY AUTOINCREMENT,
    type TEXT NOT NULL,
    url TEXT NOT NULL REFERENCES versioningEntry(url) ON DELETE CASCADE,
    id TEXT NOT NULL,
    idFrom TEXT,
    jsonObject TEXT AS JsonObject NOT NULL,
    UNIQUE(type, url, id)
);

CREATE INDEX IF NOT EXISTS idx_jsonObjectTransientEntry_type_id ON jsonObjectContextualEntry(type, id);
