{
  "material-resource": {
    "global": {
      "setting": {
        "validity-key": "1",
        "urls-white-list": "auth/"
      },
      "subs": [
        "auth/common/subs/sub-colors",
        "auth/common/subs/sub-components",
        "auth/common/subs/sub-dimensions",
        "auth/common/subs/sub-styles",
        "auth/common/subs/sub-texts",
        "auth/common/subs/sub-images"
      ],
      "templates": [
        "auth/common/templates/template-page-default"
      ]
    },
    "local": {
      "setting": {
        "validity-key": "1",
        "pre-download": true
      },
      "pages": [
        {
          "validity-key": "1",
          "url": "auth/page-home"
        },
        "auth/page-a",
        "auth/page-c",
        "auth/page-d",
        "auth/page-e"
      ]
    },
    "contextual": {
      "page-home": {
        "setting": {
          "validity-key": "1",
          "url-origin": "auth/page-home"
        },
        "definition": [
          {
            "validity-key": "1",
            "url": "${url-origin}-contextual-texts"
          },
          {
            "validity-key": "1",
            "url": "${url-origin}-contextual-components"
          }
        ]
      },
      "other": [
        {
          "validity-key": "1",
          "url-origin": "auth/page-c"
        }
      ]
    }
  }
}
