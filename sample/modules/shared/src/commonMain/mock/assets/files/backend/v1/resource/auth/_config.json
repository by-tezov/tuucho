        {
          "validity-key": "1",
          "url": "auth/page-d"
        },
        {
          "validity-key": "1",
          "url": "auth/page-e"
        }
      ]
    },
    "contextual": {
      "setting": {
        "url-origin": "auth/page-home"
      },
      "page-home": [
        {
          "validity-key": "1",
          "url": "${url-origin}-contextual-texts"
        },
        {
          "validity-key": "1",
          "url": "${url-origin}-contextual-components"
        }
      ],
      "other": [
        {
          "validity-key": "1",
          "url-origin": "auth/page-c"
        }
      ]
